<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>GoogleContactsCrawler</servlet-name>
		<servlet-class>com.ciandt.gcc.GoogleContactsCrawlerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleContactsCrawler</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Authorize</servlet-name>
		<servlet-class>com.ciandt.gcc.AuthorizeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Authorize</servlet-name>
		<url-pattern>/authorize</url-pattern>
	</servlet-mapping>

</web-app>
